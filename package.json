{
  "name": "node-js-getting-started",
  "version": "0.3.0",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "server-dry":     "node index.js",
    "server-watch":   "nodemon bin/dev",
    "server-test":    "mocha dev/service_tests/",
    "client-install": "npm install --prefix client" ,
    "client-run":     "npm run start --prefix client" ,
    "start":          "concurrently \"npm run server-watch\" \"npm run client-run\""
  },
  "dependencies": {
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "mongoose": "^5.9.5",
    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "redis": "^3.0.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "mocha": "^7.1.1"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
